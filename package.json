{
  "name": "project-jump",
  "version": "0.3.0",
  "description": "Simple Project Navigation for Atom, without the fuss.",
  "author": "Lixquid",
  "repository": "https://github.com/lixquid/atom-project-jump",
  "license": "GPL-3.0",
  "keywords": [
    "projects",
    "mavigation"
  ],
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.1.0"
  },
  "main": "./lib/project-jump",
  "activationCommands": {
    "atom-workspace": [
      "project-jump:add-multiple",
      "project-jump:add",
      "project-jump:open",
      "project-jump:remove-multiple",
      "project-jump:remove",
      "project-jump:switch"
    ]
  }
}
